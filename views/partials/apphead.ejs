<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('../partials/dependencies.ejs') %>
    <title>TIME - Motor Show App Prototype</title>
</head>
<body>
    <div class="container-xxl p-0 shadow min-vh-100 position-relative bg-main4 overflow-hidden" id="appcontainer">

    <div class="container-xxl px-0 position-fixed top-0 left-0 bg-main header" id="header" >
        <div class="d-flex justify-content-between align-items-center position-relative h-100">
            <div class="h-100 d-flex flex-column justify-content-center">
                <%if(title){%>
                    <a href="javascript:history.back()" class="box text-decoration-none fs-menu"><i class="fas fa-chevron-left text-white "></i></a>
                <%}else{%>
                    <div class="box"></div>
                <%}%>
            </div>
            <div class="text-center h-100 d-flex flex-column justify-content-center">
                <%if(title){%>
                    <div class="text-uppercase text-white fs-menu"><%= title %></div>
                <%}else{%>
                <img class="img-fluid logo " src="/app/logo_white.svg" alt="">
                <%}%>
            </div>
            <div class="w-100 min-vh-100 bg-white position-absolute top-0 start-0 menupage px-md-5 pe-none" style="opacity: 0; ">
                <div class="text-center header d-flex align-items-center justify-content-center h70">
                    <div class="text-uppercase fs-menu">Menus</div>
                </div>
                <div class="d-flex mx-auto flex-wrap menulinks overflow-auto" style="height: calc(100vh - 70px);" >
                    <%menus.map((e)=>{%>
                    <a href="<%= e.link %>" class="d-flex align-items-center col-12 col-lg-6 border-bottom">
                        <div class="icon">
                            <%if(!e?.svg){%>
                            <i class="<%= e.icon %>"></i>
                            <%}else{%>
                                <%-e?.svg-%>
                            <%}%>
                        </div>
                        <div>
                            <div class="fs-h1"><%=e.label%></div>
                            <div class="fs-small"><%=e.des%></div>
                        </div>
                    </a>
                    <%})%>
                </div>
            </div>
            <div class="h-100 d-flex flex-column justify-content-center">
                <button class="h-100 hamburger hamburger--slider-r box position-relative " type="button">
                    <span class="hamburger-box">
                    <span class="hamburger-inner"></span>
                    </span>
                </button>
            </div>
        </div>
    </div>

    <div class="header"></div>